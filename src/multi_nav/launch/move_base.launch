<launch>

    <arg name="params_file_name" default="rb_0" />
    
    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
        <rosparam file="$(find multi_nav)/param/$(arg params_file_name)/costmap_common_params.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find multi_nav)/param/$(arg params_file_name)/costmap_common_params.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find multi_nav)/param/$(arg params_file_name)/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find multi_nav)/param/$(arg params_file_name)/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find multi_nav)/param/base_local_planner_params.yaml" command="load" />
    </node>

</launch>
